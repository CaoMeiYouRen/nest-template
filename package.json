{
  "name": "nest-template",
  "version": "0.1.0",
  "description": "nest-template",
  "keywords": [
    "nest",
    "template"
  ],
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 12.9.0"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src test --fix --ext .ts,.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@typegoose/typegoose": "^11.0.0",
    "axios": "^1.3.5",
    "bcryptjs": "^2.4.3",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^6.7.0",
    "file-stream-rotator": "^1.0.0",
    "fs-extra": "^11.1.0",
    "helmet": "^6.1.2",
    "module-alias": "^2.2.2",
    "mongoose": "^6.5.5",
    "morgan": "^1.10.0",
    "nestjs-mongoose-crud": "^2.1.2",
    "nestjs-typegoose-next": "^0.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.2",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^28.1.8",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "5.58.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.38.0",
    "eslint-config-cmyr": "1.1.28",
    "gulp": "^4.0.2",
    "jest": "^28.1.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "@types/superagent": "4.1.14",
      "@typegoose/typegoose": "9.12.1",
      "mongodb": "4.9.1",
      "mongoose": "6.6.3"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/mongoose",
        "express",
        "mongodb",
        "webpack"
      ]
    }
  },
  "overrides": {
    "@types/superagent": "4.1.14",
    "@typegoose/typegoose": "9.12.1",
    "mongodb": "4.9.1",
    "mongoose": "6.6.3"
  },
  "resolutions": {
    "@types/superagent": "4.1.14",
    "@typegoose/typegoose": "9.12.1",
    "mongodb": "4.9.1",
    "mongoose": "6.6.3"
  }
}
