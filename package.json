{
  "name": "nest-template",
  "version": "0.1.0",
  "description": "nest-template",
  "keywords": [
    "nest",
    "template"
  ],
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.2",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/swagger": "^4.5.12",
    "@typegoose/typegoose": "^7.3.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "fs-extra": "^9.0.1",
    "got": "^11.5.2",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "mongoose-long": "^0.3.1",
    "nestjs-mongoose-crud": "^2.0.0",
    "nestjs-typegoose": "^7.1.32",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.3.2",
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.4",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^14.10.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-cmyr": "^0.7.1",
    "gulp": "^4.0.2",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "supertest": "^5.0.0",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
