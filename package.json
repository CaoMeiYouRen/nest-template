{
  "name": "nest-template",
  "version": "0.1.0",
  "description": "nest-template",
  "keywords": [
    "nest",
    "template"
  ],
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "private": true,
  "main": "dist/main.js",
  "engines": {
    "node": ">= 12.9.0"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src test --fix --ext .ts,.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.14",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.14",
    "@nestjs/platform-express": "^11.1.14",
    "@nestjs/swagger": "^11.2.6",
    "@typegoose/typegoose": "^13.1.0",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dayjs": "^1.11.19",
    "dotenv": "^17.3.1",
    "express-rate-limit": "^8.2.1",
    "file-stream-rotator": "^1.0.0",
    "fs-extra": "^11.3.3",
    "helmet": "^8.1.0",
    "module-alias": "^2.2.3",
    "mongodb": "^7.1.0",
    "mongoose": "8.9.5",
    "morgan": "^1.10.1",
    "nestjs-mongoose-crud": "^2.1.2",
    "nestjs-typegoose-next": "^0.1.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.3",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.14",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.3.0",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "debug": "^4.4.3",
    "eslint": "^9.39.2",
    "eslint-config-cmyr": "2.1.0",
    "gulp": "^5.0.1",
    "jest": "^30.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".(spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "@types/superagent": "^8.1.0",
      "@typegoose/typegoose": "$@typegoose/typegoose",
      "mongodb": "$mongodb",
      "mongoose": "$mongoose",
      "formidable": "^3.2.4",
      "fast-xml-parser": "^4.4.1",
      "cookie": "^0.7.0",
      "@babel/helpers": "^7.26.10",
      "brace-expansion": "^2.0.2",
      "tmp": "^0.2.4",
      "validator": "^13.15.20",
      "glob": "^11.1.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/mongoose",
        "express",
        "mongodb",
        "webpack"
      ]
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf"
    ]
  }
}
