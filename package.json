{
  "name": "nest-template",
  "version": "0.1.0",
  "description": "nest-template",
  "keywords": [
    "nest",
    "template"
  ],
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src test --fix --ext .ts,.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@typegoose/typegoose": "^9.8.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "colors": "^1.4.0",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "express-rate-limit": "^6.4.0",
    "file-stream-rotator": "^0.6.1",
    "fs-extra": "^10.1.0",
    "got": "^12.1.0",
    "helmet": "^5.1.0",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.8",
    "mongoose-long": "^0.5.1",
    "morgan": "^1.10.0",
    "nestjs-mongoose-crud": "^2.1.2",
    "nestjs-typegoose": "^7.1.38",
    "nestjs-typegoose-next": "^0.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.20.0",
    "eslint-config-cmyr": "^1.1.21",
    "gulp": "^4.0.2",
    "jest": "^28.1.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "@types/superagent": "4.1.14"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/mongoose",
        "express",
        "mongodb",
        "webpack"
      ]
    }
  }
}
