{
  "name": "nest-template",
  "version": "0.1.0",
  "description": "nest-template",
  "keywords": [
    "nest",
    "template"
  ],
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "private": true,
  "main": "dist/main.js",
  "engines": {
    "node": ">= 12.9.0"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src test --fix --ext .ts,.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.13",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/swagger": "^8.1.0",
    "@typegoose/typegoose": "^12.9.1",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express-rate-limit": "^7.4.1",
    "file-stream-rotator": "^1.0.0",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.11.0",
    "mongoose": "8.8.3",
    "morgan": "^1.10.0",
    "nestjs-mongoose-crud": "^2.1.2",
    "nestjs-typegoose-next": "^0.1.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^7.0.3",
    "debug": "^4.4.0",
    "eslint": "^8.57.1",
    "eslint-config-cmyr": "^1.1.32",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".(spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "@types/superagent": "^8.1.0",
      "@typegoose/typegoose": "$@typegoose/typegoose",
      "mongodb": "$mongodb",
      "mongoose": "$mongoose",
      "formidable": "^3.2.4",
      "fast-xml-parser": "^4.4.1",
      "cookie": "^0.7.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/mongoose",
        "express",
        "mongodb",
        "webpack"
      ]
    }
  }
}